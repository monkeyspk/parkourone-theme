{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "parkourone/testimonials-slider",
	"version": "1.2.0",
	"title": "Testimonials Slider",
	"category": "parkourone-startseite",
	"icon": "format-quote",
	"description": "Horizontaler Slider mit Kundenbewertungen - intelligent nach Seitentyp",
	"keywords": ["testimonials", "bewertungen", "reviews", "slider", "google", "altersgruppen"],
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},
	"attributes": {
		"headline": {
			"type": "string",
			"default": "Erfahrungen unserer Teilnehmer_innen:"
		},
		"showStars": {
			"type": "boolean",
			"default": true
		},
		"filterAgeGroup": {
			"type": "string",
			"default": ""
		},
		"pageType": {
			"type": "string",
			"default": ""
		},
		"limit": {
			"type": "number",
			"default": 3
		},
		"showGoogleCard": {
			"type": "boolean",
			"default": true
		},
		"googleReviewsUrl": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "parkourone",
	"editorScript": "file:./index.js",
	"viewScript": "file:./view.js",
	"style": "file:./style.css",
	"render": "file:./render.php"
}
