{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "parkourone/faq",
	"version": "1.1.0",
	"title": "FAQ Accordion",
	"category": "parkourone-blocks",
	"icon": "editor-help",
	"description": "Häufig gestellte Fragen als Accordion mit Schema.org SEO-Markup",
	"keywords": ["faq", "fragen", "accordion", "hilfe", "support"],
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},
	"attributes": {
		"headline": {
			"type": "string",
			"default": "Häufige Fragen"
		},
		"category": {
			"type": "string",
			"default": ""
		},
		"limit": {
			"type": "number",
			"default": 6
		},
		"includeGeneral": {
			"type": "boolean",
			"default": false
		},
		"backgroundColor": {
			"type": "string",
			"default": "white"
		}
	},
	"textdomain": "parkourone",
	"editorScript": "file:./index.js",
	"style": "file:./style.css",
	"render": "file:./render.php"
}
